<Project>
    <PropertyGroup Condition=" '$(ProjectName)' != '_build' ">
        <TargetFramework>net48</TargetFramework>
        <LangVersion>10</LangVersion>
        <PlatformTarget>x64</PlatformTarget>
        <IsPackable>false</IsPackable>
        <CodeAnalysisRuleSet>..\stylecop.ruleset</CodeAnalysisRuleSet>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <!-- <ItemGroup Condition=" '$(ProjectName)' != '_build' And !'$(ProjectName.EndsWith(`Tests`))'">
       <PackageReference Include="StyleCop.Analyzers" Version="1.2.0-beta.435">
         <PrivateAssets>all</PrivateAssets>
         <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
       </PackageReference>
     </ItemGroup>-->
    <PropertyGroup>
        <MsiFilePrefix>ElectricityRevitPlugin.</MsiFilePrefix>
        <ProductVersion>2022</ProductVersion>
        <Authors>Ivachevev</Authors>
        <Product>ElectricityRevitPlugin</Product>
        <Copyright>Copyright Â©2022</Copyright>
        <PackageTags>BIM ElectricityRevitPlugin</PackageTags>
    </PropertyGroup>
    <ItemGroup Condition=" '$(UseCommand)' == 'true' ">
        <Reference Include="RevitAPI">
            <HintPath>..\ref\RevitAPI.dll</HintPath>
            <Private>False</Private>
        </Reference>
        <Reference Include="RevitAPIIFC">
            <HintPath>..\ref\RevitAPIIFC.dll</HintPath>
            <Private>False</Private>
        </Reference>
        <Reference Include="RevitAPIMacros">
            <HintPath>..\ref\RevitAPIMacros.dll</HintPath>
            <Private>False</Private>
        </Reference>
        <Reference Include="RevitAPIUI">
            <HintPath>..\ref\RevitAPIUI.dll</HintPath>
            <Private>False</Private>
        </Reference>
        <Reference Include="RevitAPIUIMacros">
            <HintPath>..\ref\RevitAPIUIMacros.dll</HintPath>
            <Private>False</Private>
        </Reference>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseApplication)' == 'true' ">
        <PackageReference Include="RxBim.Application.Revit" Version="2019.1.0.7"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseRibbonMenu)' == 'true' ">
        <PackageReference Include="PikTools.Application.Ribbon.About.Revit" Version="1.0.2"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseLogs)' == 'true' ">
        <PackageReference Include="RxBim.Logs.Revit" Version="2019.1.0.7"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseRevitTools)' == 'true' ">
        <PackageReference Include="RxBim.Tools.Revit" Version="2019.3.3"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseSimpleInjector)' == 'true' ">
        <PackageReference Include="RxBim.Di.SimpleInjector" Version="1.1.0"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseLogWindow)' == 'true' ">
        <PackageReference Include="PikTools.LogWindow" Version="1.0.10"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UsePikUi)' == 'true' ">
        <PackageReference Include="PikTools.Ui" Version="3.3.3"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseTableBuilder)' == 'true' ">
        <PackageReference Include="RxBim.Tools.TableBuilder.Excel" Version="1.0.1"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseSharedTools)' == 'true' ">
        <PackageReference Include="PikTools.Shared" Version="1.0.0"/>
    </ItemGroup>
    <ItemGroup Condition=" '$(UseMoreLinq)' == 'true' ">
        <PackageReference Include="morelinq" Version="3.3.2"/>
    </ItemGroup>
</Project>